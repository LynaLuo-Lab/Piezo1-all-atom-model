integrator {
    Multigrator {
        barostat {
            MTK {
                T_ref = "310.15"
                tau = "0.0416667"
                thermostat {
                    NoseHoover {
                        chain {
                            mts = "4"
                            tau = ["0.0416667" "0.0416667" "0.0416667"]
                        }
                    }
                    type = "NoseHoover"
                }
            }
            interval = "480"
            type = "MTK"
        }
        thermostat {
            NoseHoover {
                chains = [{
                    mts = "1"
                    tau = [".0416667" ".0416667" ".0416667"]
                }]
                use_molecular_ke = "true"
            }
            interval = "24"
            type = "NoseHoover"
        }
    }
    dt = ".0020"
    pressure {
        isotropy = "semi_isotropic"
        p_ref = "1.0"
        tension_ref = ["_TENSION_" "_TENSION_" "_TENSION_" "_TENSION_" "_TENSION_" "_TENSION_" "_TENSION_" "_TENSION_" "_TENSION_"]
    }
    remove_com_motion = "true"
